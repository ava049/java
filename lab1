public class TriangleFromSimpleGeometricObject{
  extends SimpleGeometricObject;
  private double  sidea = 1;
  private double  sideb = 1;
  private double  sidec = 1;
  //private double  anglea;
  //private double  angleb;
  //private double  anglec;

  public TriangleFromSimpleGeometricObject(){
  }

  public  TriangleFromSimpleGeometricObject(double const_sidea, double const_sideb, double const_sidec, boolean const_filled, string const_color){
    super(const_color, const_filled);
    //setColor(const_color);
    //setFilled(const_filled);
    sidea = const_sidea;
    sideb = const_sideb;
    sidec = const_sidec;}

  public  double  getArea(){
    double s = (sidea+sideb+sidec)/2;
    double area = Math.sqrt(s*(s-sidea)*(s-sideb)*(s-sidec))
    return  area;}

  public  string getPerimeter(){
    return  sidea + sideb + sidec;}

  public  string  toString(){
    return  super.toString()+" Dette er siden til trekanten: "+sidea+", "+sideb" og "+sidec+". Dette er arealet: "+getArea();
  }
